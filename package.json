{
    "name": "better-mips",
    "displayName": "Better MIPS Support",
    "description": "Provides better syntax highlighting and snippets for MIPS assembly language",
    "version": "0.2.1",
    "publisher": "vasilescur",
    "icon": "images/logo.png",
    "homepage": "https://github.com/vasilescur/vscode-Better-MIPS",
    "repository": {
        "type": "git",
        "url": "https://github.com/vasilescur/vscode-Better-MIPS"
    },
    "bugs": {
        "url": "https://github.com/vasilescur/vscode-Better-MIPS/issues",
        "email": "vasilescur@gmail.com"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Snippets",
        "Themes"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mips",
                "aliases": [
                    "MIPS Assembly",
                    "mips"
                ],
                "extensions": [
                    ".s",
                    ".mips",
                    ".spim",
                    ".asm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mips",
                "scopeName": "source.mips",
                "path": "./syntaxes/mips.tmLanguage"
            }
        ],
        "themes": [
            {
                "label": "Nord MIPS",
                "uiTheme": "vs-dark",
                "path": "./themes/Nord MIPS-color-theme.json"
            }
        ]
    }
}
